@model SHIVAMFaceEcomm.Models.ProductDetail
@{
    ViewBag.Title = "ProductDetail";
    var allAttributes = Model.Attributes.Select(p => p.AttributeName).Distinct().ToList();
}


<div class="container" style="display:none;">
    <br>
    <div class="page-header">
        <h3>Product Detail for @Model.Product.ProductName</h3>
    </div>
    <hr>

    <div class="card">
        <div class="row">
            <aside class="col-sm-5 border-right">
                <article class="gallery-wrap">
                    <div class="img-big-wrap">
                        <div> <a href="#"><img src="http://localhost:14988/ProductImages/@Model.Attributes.First().ImagePath" onerror="this.onerror = null; this.src = '/Content/images/no-image.png';" width="500px" height="100%"></a></div>
                    </div> <!-- slider-product.// -->
                    <div class="img-small-wrap">
                        <div class="row">
                            @for (int i = 0; i < Model.Attributes.Count(); i++)
                            {
                                <div class="column">
                                    <img src="http://localhost:14988/ProductImages/@Model.Attributes[i].ImagePath" onerror="this.onerror = null; this.src = '/Content/images/no-image.png';" style="width:100%" onclick="openModal();currentSlide(@(i+1))" class="hover-shadow cursor">
                                </div>

                            }


                        </div>


                    </div> <!-- slider-nav.// -->



                    <div id="myModal" class="modal">
                        <span class="close cursor" onclick="closeModal()">&times;</span>
                        <div class="modal-content">
                            @for (int i = 0; i < Model.Attributes.Count(); i++)
                            {
                                <div class="mySlides">
                                    <div class="numbertext">@(i + 1) / @Model.Attributes.Count()</div>
                                    <img src="http://localhost:14988/ProductImages/@Model.Attributes[i].ImagePath" onerror="this.onerror = null; this.src = '/Content/images/no-image.png';" style="width:100%">
                                </div>

                            }




                            <a class="prev" onclick="plusSlides(-1)">&#10094;</a>
                            <a class="next" onclick="plusSlides(1)">&#10095;</a>

                            <div class="caption-container">
                                <p id="caption"></p>
                            </div>

                            @for (int i = 0; i < Model.Attributes.Count(); i++)
                            {
                                <div class="column">
                                    <img class="demo cursor" src="http://localhost:14988/ProductImages/@Model.Attributes[i].ImagePath" onerror="this.onerror = null; this.src = '/Content/images/no-image.png';" style="width:100%" onclick="currentSlide(@(i+1))" alt="@Model.Attributes[i].ImageName">
                                </div>
                            }

                        </div>
                    </div>
                </article> <!-- gallery-wrap .end// -->
            </aside>
            <aside class="col-sm-7">
                <article class="card-body p-5">
                    <h3 class="title mb-3">@Model.Product.ProductName</h3>
                    <p class="price-detail-wrap">
                        <span class="price h3 text-warning">
                            <span class="currency">US $</span><span class="num">1299</span>
                        </span>
                        <span>/per kg</span>
                    </p> <!-- price-detail-wrap .// -->
                    <dl class="item-property">
                        <dt>Description</dt>
                        <dd>
                            <p>
                                @Model.Product.Description
                            </p>
                        </dd>
                    </dl>
                    <dl class="param param-feature">
                        <dt>Model#</dt>
                        <dd>@Model.Product.SKU</dd>
                    </dl>  <!-- item-property-hor .// -->
                    @for (int i = 0; i < allAttributes.Count(); i++)
                    {
                        <dl class="param param-feature">
                            <dt>@allAttributes[i]</dt>
                            @{
                        var allattributeV = Model.Attributes.Where(p => p.AttributeName == allAttributes[i]).Select(p => p.AttributeValue).Distinct().ToList();
                            }
                            <dd>
                                <select class="form-control form-control-sm" id="attribute@(i+1)" onchange="GetAttributeDate(@allAttributes[i])" attributename="@allAttributes[i]" style="width:70px;">
                                    @for (int k = 0; k < allattributeV.Count(); k++)
                                    {
                                        <option value="@allattributeV[k]"> @allattributeV[k] </option>
                                    }


                                </select>
                            </dd>
                        </dl>  <!-- item-property-hor .// -->
                    }


                    <hr>
                    <div class="row">
                        <div class="col-sm-12">
                            <dl class="param param-inline">
                                <dt>Quantity: </dt>
                                <dd>
                                    <select class="form-control form-control-sm">
                                        <option> 1 </option>
                                        <option> 2 </option>
                                        <option> 3 </option>
                                    </select>
                                </dd>
                            </dl>  <!-- item-property .// -->
                        </div> <!-- col.// -->
                        <!-- col.// -->
                    </div> <!-- row.// -->
                    <hr>
                    <a href="#" class="btn btn-lg btn-primary text-uppercase" > Buy now </a>

                    <a href="#" class="btn btn-lg btn-outline-primary text-uppercase"> <i class="fas fa-shopping-cart"></i> Add to cart </a>
                </article> <!-- card-body.// -->
            </aside> <!-- col.// -->
        </div> <!-- row.// -->
    </div> <!-- card.// -->

</div>
